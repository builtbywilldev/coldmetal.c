    "vector": [-0.343203, -0.147087, 0.049029, 0.049029, -0.049029, -0.049029, -0.245145, -0.147087, 0.049029, -0.049029, 0.147087, 0.049029, 0.147087, 0.147087, 0.049029, -0.049029, -0.245145, 0.049029, -0.049029, 0.049029, -0.049029, -0.049029, -0.147087, -0.049029, 0.049029, -0.147087, -0.049029, -0.049029, -0.049029, 0.049029, 0.049029, -0.245145, -0.343203, -0.147087, 0.049029, 0.049029, -0.049029, -0.049029, -0.245145, -0.147087, 0.049029, -0.049029, 0.147087, 0.049029, 0.147087, 0.147087, 0.049029, -0.049029, -0.245145, 0.049029, -0.049029, 0.049029, -0.049029, -0.049029, -0.147087, -0.049029, 0.049029, -0.147087, -0.049029, -0.049029, -0.049029, 0.049029, 0.049029, -0.245145],
