    "vector": [-0.068843, -0.068843, 0.068843, 0.034421, 0.068843, -0.068843, -0.034421, -0.068843, 0.034421, 0.068843, 0.068843, -0.103264, 0.137686, 0.000000, 0.206529, 0.034421, 0.103264, -0.103264, 0.000000, 0.103264, 0.137686, -0.206529, -0.103264, -0.068843, -0.172107, -0.137686, -0.103264, 0.068843, 0.000000, -0.240950, -0.240950, -0.309793, -0.068843, -0.068843, 0.068843, 0.034421, 0.068843, -0.068843, -0.034421, -0.068843, 0.034421, 0.068843, 0.068843, -0.103264, 0.137686, 0.000000, 0.206529, 0.034421, 0.103264, -0.103264, 0.000000, 0.103264, 0.137686, -0.206529, -0.103264, -0.068843, -0.172107, -0.137686, -0.103264, 0.068843, 0.000000, -0.240950, -0.240950, -0.309793],
    "label": "U"
