    "vector": [-0.123718, 0.041239, 0.164957, -0.123718, -0.041239, -0.041239, -0.206197, -0.082479, 0.000000, 0.000000, 0.082479, 0.041239, 0.164957, 0.206197, 0.041239, -0.082479, 0.123718, 0.082479, -0.041239, -0.041239, -0.123718, -0.123718, -0.082479, -0.082479, -0.041239, -0.082479, -0.164957, -0.041239, -0.082479, -0.206197, -0.288675, -0.247436, -0.123718, 0.041239, 0.164957, -0.123718, -0.041239, -0.041239, -0.206197, -0.082479, 0.000000, 0.000000, 0.082479, 0.041239, 0.164957, 0.206197, 0.041239, -0.082479, 0.123718, 0.082479, -0.041239, -0.041239, -0.123718, -0.123718, -0.082479, -0.082479, -0.041239, -0.082479, -0.164957, -0.041239, -0.082479, -0.206197, -0.288675, -0.247436],
    "label": "U"
