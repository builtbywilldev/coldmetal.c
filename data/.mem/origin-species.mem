    "vector": [0.046044, -0.082880, 0.027627, 0.064462, 0.101298, -0.119715, -0.101298, -0.064462, 0.064462, 0.174968, -0.082880, -0.101298, 0.027627, 0.138133, 0.101298, -0.027627, -0.138133, -0.101298, -0.082880, 0.193386, 0.046044, -0.193386, -0.119715, 0.027627, -0.193386, -0.156551, -0.046044, 0.156551, 0.064462, -0.027627, 0.009209, -0.377564, 0.046044, -0.082880, 0.027627, 0.064462, 0.101298, -0.119715, -0.101298, -0.064462, 0.064462, 0.174968, -0.082880, -0.101298, 0.027627, 0.138133, 0.101298, -0.027627, -0.138133, -0.101298, -0.082880, 0.193386, 0.046044, -0.193386, -0.119715, 0.027627, -0.193386, -0.156551, -0.046044, 0.156551, 0.064462, -0.027627, 0.009209, -0.377564],
    "label": "U"
