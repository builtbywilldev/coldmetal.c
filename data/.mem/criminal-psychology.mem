    "vector": [-0.057092, -0.034255, -0.074219, -0.022837, -0.062801, -0.005709, -0.062801, -0.074219, -0.011418, 0.017127, -0.028546, 0.125602, 0.097056, 0.074219, 0.137020, -0.119892, 0.000000, -0.171275, -0.159857, 0.194111, 0.068510, -0.245494, -0.102765, 0.045673, -0.171275, -0.114183, -0.142729, 0.005709, -0.085637, -0.188402, -0.171275, -0.325422, -0.057092, -0.034255, -0.074219, -0.022837, -0.062801, -0.005709, -0.062801, -0.074219, -0.011418, 0.017127, -0.028546, 0.125602, 0.097056, 0.074219, 0.137020, -0.119892, 0.000000, -0.171275, -0.159857, 0.194111, 0.068510, -0.245494, -0.102765, 0.045673, -0.171275, -0.114183, -0.142729, 0.005709, -0.085637, -0.188402, -0.171275, -0.325422],
    "label": "U"
