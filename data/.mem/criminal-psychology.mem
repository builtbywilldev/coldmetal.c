    "vector": [-0.098295, -0.049147, 0.000000, -0.098295, 0.122868, -0.122868, -0.098295, 0.049147, 0.049147, 0.024574, 0.024574, -0.049147, 0.049147, 0.098295, 0.098295, 0.024574, -0.073721, -0.196589, -0.122868, 0.147442, -0.024574, -0.245737, -0.122868, 0.073721, 0.049147, 0.000000, -0.098295, 0.147442, -0.147442, -0.221163, -0.196589, -0.319458, -0.098295, -0.049147, 0.000000, -0.098295, 0.122868, -0.122868, -0.098295, 0.049147, 0.049147, 0.024574, 0.024574, -0.049147, 0.049147, 0.098295, 0.098295, 0.024574, -0.073721, -0.196589, -0.122868, 0.147442, -0.024574, -0.245737, -0.122868, 0.073721, 0.049147, 0.000000, -0.098295, 0.147442, -0.147442, -0.221163, -0.196589, -0.319458],
    "label": "U"
