  {
    "text": "TOPIC IV. ISOLATED SPECIAL CONDITIONS.",
    "vector": [-0.176777, 0.058926, 0.176777, -0.058926, -0.058926, -0.294628, -0.058926, 0.058926, -0.058926, -0.058926, -0.058926, -0.058926, -0.058926, 0.176777, 0.176777, -0.176777, -0.058926, 0.058926, -0.058926, -0.058926, 0.058926, -0.058926, 0.176777, -0.058926, -0.058926, -0.176777, -0.176777, -0.176777, -0.058926, -0.176777, -0.176777, -0.058926, -0.176777, 0.058926, 0.176777, -0.058926, -0.058926, -0.294628, -0.058926, 0.058926, -0.058926, -0.058926, -0.058926, -0.058926, -0.058926, 0.176777, 0.176777, -0.176777, -0.058926, 0.058926, -0.058926, -0.058926, 0.058926, -0.058926, 0.176777, -0.058926, -0.058926, -0.176777, -0.176777, -0.176777, -0.058926, -0.176777, -0.176777, -0.058926],
    "label": "U"
