    "vector": [0.000000, 0.000000, 0.000000, 0.000000, 0.196116, -0.196116, -0.196116, 0.000000, 0.000000, 0.000000, 0.196116, 0.196116, 0.000000, 0.000000, 0.000000, 0.000000, 0.000000, 0.000000, 0.196116, 0.000000, -0.196116, 0.000000, -0.196116, 0.000000, 0.000000, 0.196116, -0.196116, 0.196116, 0.000000, 0.000000, -0.196116, -0.196116, 0.000000, 0.000000, 0.000000, 0.000000, 0.196116, -0.196116, -0.196116, 0.000000, 0.000000, 0.000000, 0.196116, 0.196116, 0.000000, 0.000000, 0.000000, 0.000000, 0.000000, 0.000000, 0.196116, 0.000000, -0.196116, 0.000000, -0.196116, 0.000000, 0.000000, 0.196116, -0.196116, 0.196116, 0.000000, 0.000000, -0.196116, -0.196116],
